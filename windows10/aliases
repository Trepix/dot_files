   alias ls="ls -lFgh --color=auto"
   alias lls="ls -laFgh --color=auto"
   alias tree="tree -L 1"
   alias tree2="tree -L 2"
   alias tree3="tree -L 3"
   alias tree4="tree -L 4"
   alias tree5="tree -L 5"
   alias graph="git log --graph --decorate --all"
   alias diff="diff -yBb --suppress-common-lines"
   alias python="python3"